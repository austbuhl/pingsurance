<h1>Pingsurance</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Date Texted</th>
    </tr>
  </thead>
  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.full_name %></td>
        <td><%= lead.phone_number %></td>
        <td><%= lead.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>